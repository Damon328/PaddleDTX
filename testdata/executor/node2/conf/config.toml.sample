[executor]
name = "exe-node2"
listenAddress = ":80"
publicAddress = "executor2.node.com:80"
privateKey = "EXECUTOR2PRIVATEKEY"

[executor.mpc]
    trainTaskLimit = 100
    predictTaskLimit = 100
    rpcTimeout = 3
    # task maximum execution time (in seconds)
    taskLimitTime = 3600

[executor.storage]
    localStoragePath = "./models"
    type = 'XuperDB'
    [executor.storage.XuperDB]
        host = "http://dataowner2.node.com:80"
        namespace = "FILESNAMESPACES"
        expiretime = 72


[executor.blockchain]
    type = 'xchain'
    [executor.blockchain.xchain]
        mnemonic = "MNEMONIC"
        contractName = "CONTRACTNAME"
        contractAccount = "XCCONTRACTACCOUNT@xuper"
        chainAddress = "xchain1.node.com:37101"
        chainName = "xuper"

[log]
level = "debug"
path = "./logs"
